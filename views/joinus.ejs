      <div class="form-group">
        <label for="name" style="color: aliceblue ; font-size: 20px;">Full Name</label>
        <input type="text" name="Namej" id="name" placeholder="First and Last" required minlength="3" maxlength="25" />
      </div>
      <div class="form-group">
        <label for="email" style="color: aliceblue ; font-size: 20px;">Email Address</label>
        <input type="email" name="Emailj" id="email" placeholder="email@domain.tld" required />
      </div>
      <div class="form-group">
        <label for="message" style="color: aliceblue ; font-size: 20px;">Address</label>
        <textarea name="Messagej" id="message" rows="5" placeholder="Type your message here...."></textarea>
      </div>
      <div class="form-group">
        <label for="message" style="color: aliceblue ; font-size: 20px;">Profession</label>
        <input type="text" name="professionj" id="name" placeholder="Type here" required minlength="3" maxlength="25" />
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-secondary"><i class="far fa-paper-plane"></i>Send</button>
      </div>
    </form>
